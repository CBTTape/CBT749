//GILBERTH JOB (ACCT#),HANDBOOK,                                 OS/VS2
// NOTIFY=&SYSUID,
// CLASS=A,MSGCLASS=X,COND=(0,NE)
//*
//*    THIS JOB ASSEMBLES MVS CONTROL BLOCK MACROS FROM
//*    SYS1.MACLIB AND SYS1.AMODGEN AND STORES OUTPUT
//*    LISTINGS INTO PDS MEMBERS FOR ON-LINE VIEWING.
//*
//CLEANUP EXEC PGM=IEFBR14
//SYSPRINT DD DSN=GILBERT.HANDBOOK.OSVS2,
// DISP=(MOD,DELETE),UNIT=SYSALLDA,SPACE=(TRK,0)
//*
//ALLOCATE EXEC PGM=IEFBR14
//SYSPRINT DD DSN=GILBERT.HANDBOOK.OSVS2,
// DISP=(NEW,CATLG,DELETE),
// UNIT=SYSALLDA,
// SPACE=(TRK,(50,15,5),RLSE),
// DCB=(RECFM=FBM,LRECL=121,BLKSIZE=12100)
//SYSUT1   DD DSN=&&SYSUT1,DISP=(,PASS),UNIT=VIO,SPACE=(CYL,20)
//SYSUT2   DD DSN=&&SYSUT2,DISP=(,PASS),UNIT=VIO,SPACE=(CYL,20)
//SYSUT3   DD DSN=&&SYSUT3,DISP=(,PASS),UNIT=VIO,SPACE=(CYL,20)
//*
//ASMMAC PROC MBR=
//TMP     EXEC PGM=IFOX00,REGION=5M,
// PARM='NOESD,NORLD,NOOBJECT,NODECK,LINECOUNT(120),TERM'
//SYSTSPRT DD SYSOUT=*
//SYSTSIN  DD DUMMY
//*
//SYSLIB   DD DSN=SYS1.MACLIB,DISP=SHR
//         DD DSN=SYS1.AMODGEN,DISP=SHR
//SYSUT1   DD DSN=&&SYSUT1,DISP=(OLD,PASS)
//SYSUT2   DD DSN=&&SYSUT2,DISP=(OLD,PASS)
//SYSUT3   DD DSN=&&SYSUT3,DISP=(OLD,PASS)
//SYSTERM  DD SYSOUT=*
//*
//SYSPRINT DD DSN=GILBERT.HANDBOOK.OSVS2(&MBR),DISP=OLD
//        PEND
//*
//ACB    EXEC ASMMAC,MBR=ACB
         IFGACB
         END
//ACEE   EXEC ASMMAC,MBR=ACEE
         IHAACEE
         END
  //AMVT   EXEC ASMMAC,MBR=ASMVT
           ILRASMVT
           END
//ASCB   EXEC ASMMAC,MBR=ASCB
         IHAASCB DSECT=YES
         END
//ASVT   EXEC ASMMAC,MBR=ASVT
         IHAASVT DSECT=YES
         END
  //ASSB   EXEC ASMMAC,MBR=ASSB
           IHAASSB LIST=YES
           END
//ASXB   EXEC ASMMAC,MBR=ASXB
         IHAASXB DSECT=YES
         END
  //ATTACH EXEC ASMMAC,MBR=ATTACH
           IEZATTCH LIST=YES
           END
//BASEA  EXEC ASMMAC,MBR=BASEA
